{
  "name": "@peterbe/gg",
  "module": "index.ts",
  "version": "0.0.14",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/peterbe/gg2"
  },
  "scripts": {
    "lint": "biome check --write --unsafe",
    "lint:check": "biome check",
    "build": "bun build src/index.ts --target=bun --outfile out/gg --compile --bytecode",
    "build:linux-arm64": "bun build src/index.ts --target=bun-linux-arm64 --outfile out/gg-linux-arm64 --compile --bytecode",
    "build:linux-x64": "bun build src/index.ts --target=bun-linux-x64 --outfile out/gg-linux-x64 --compile --bytecode",
    "prepare": "husky",
    "test": "bun test",
    "release": "np --yolo",
    "postinstall": "bunx biome migrate --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "@types/bun": "1.3.9",
    "husky": "^9.1.7",
    "np": "^10.3.0",
    "vitepress": "^2.0.0-alpha.16"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "commander": "^14.0.3",
    "fuzzysort": "^3.1.0",
    "kleur": "^4.1.5",
    "octokit": "^5.0.5",
    "simple-git": "^3.31.1",
    "yocto-spinner": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
